{
  "name": "rheinkultur-wawi",
  "version": "1.81.1",
  "private": true,
  "description": "Rheinkultur WaWi Parse Server",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mammutmedia/rheinkultur-wawi-parse.git"
  },
  "engines": {
    "node": ">= 18 < 19"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "ci:checkNodeEngine": "node ./ci/nodeEngineCheck.js",
    "start": "export TZ=\"Europe/Berlin\" && node src/index.js --trace-warnings",
    "dev": "cross-env NODE_ENV=development TZ=\"Europe/Berlin\" nodemon --inspect --watch src src/index.js",
    "dev-start": "cross-env NODE_ENV=development TZ=\"Europe/Berlin\" node src/index.js",
    "serve-json": "node serve-json.js",
    "lint": "eslint './**/*.js' --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.9.0",
    "@faker-js/faker": "7.6.0",
    "@googlemaps/google-maps-services-js": "3.3.35",
    "@parse/s3-files-adapter": "2.2.0",
    "bull": "4.10.4",
    "csvtojson": "2.0.10",
    "dotenv": "16.3.1",
    "exceljs": "4.3.0",
    "express": "4.18.2",
    "express-prom-bundle": "6.6.0",
    "express-winston": "4.2.0",
    "googleapis": "110.0.0",
    "html-to-text": "9.0.5",
    "ioredis": "5.3.2",
    "merge-base64": "1.0.4",
    "module-alias": "2.2.3",
    "moment": "2.29.4",
    "ndjson": "2.0.0",
    "onesignal-node": "3.4.0",
    "parse-server": "5.4.3",
    "parse-smtp-template": "2.2.2",
    "pdf2pic": "2.2.4",
    "proj4": "2.9.0",
    "redis-info": "3.1.0",
    "require-dir": "1.2.0",
    "sharp": "0.31.3",
    "slugify": "1.6.6",
    "winston": "3.10.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@actions/core": "1.10.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "10.0.4",
    "conventional-changelog-eslint": "3.0.9",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-rk-lint": "file:rk-lint",
    "eslint-plugin-standard": "5.0.0",
    "husky": "8.0.3",
    "log-update": "^5.0.1",
    "nodemon": "2.0.22",
    "parse-dashboard": "5.1.0",
    "semantic-release": "21.0.7"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
